<Project Sdk="Microsoft.NET.Sdk;Microsoft.NET.Sdk.Publish">
	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<SatelliteResourceLanguages>en</SatelliteResourceLanguages>
		<SatelliteResourceLanguages>tr</SatelliteResourceLanguages>
		<GenerateDocumentationFile>False</GenerateDocumentationFile>
		<SignAssembly>False</SignAssembly>
		<Copyright>Ceres Software &amp; Consultancy</Copyright>
		<PackageProjectUrl>https://merchanter.net</PackageProjectUrl>
		<AssemblyVersion>1.2.0.0</AssemblyVersion>
		<FileVersion>1.2.0.0</FileVersion>
	</PropertyGroup>
	<ItemGroup>
		<ProjectReference Include="..\MerchanterHelper\MerchanterHelper.csproj" />
		<ProjectReference Include="..\Merchanter\Merchanter.csproj" />
	</ItemGroup>
</Project>